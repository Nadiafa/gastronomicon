<h6>VIEWS/SESSIONS/new.html.erb</h6>
<h3>Please enter your details below to Log In</h3>
<%= form_tag(login_path) do %>
  <%= label_tag :username %>
  <%= text_field_tag :username %>
  <br>
  <%= label_tag :password %>
  <%= password_field_tag :password %>
  <br><br>
  <%= submit_tag 'Log In' %>
<% end %>